# 数据库配置
# Database host address
DB_HOST=localhost

# Database port number
DB_PORT=5432

# Database user
DB_USER=postgres

# Database password (生产环境请使用强密码)
DB_PASSWORD=your_password_here

# Database name
DB_NAME=mygoproject

# 消息队列配置
# RabbitMQ connection URL
# 格式: amqp://username:password@host:port/
MQ_URL=amqp://guest:guest@localhost:5672/

# JWT配置
# JWT secret key (生产环境请使用强随机字符串)
JWT_SECRET=your_jwt_secret_here

# 服务器配置
# Server port (格式: :8080 或 8080)
SERVER_PORT=:8080

